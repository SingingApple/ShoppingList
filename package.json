{
  "name": "shoppinglist",
  "version": "1.0.0",
  "description": "Shopping list built with MERN ",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon server.js",
