    "url": "https://github.com/SingingApple/ShoppingList/issues"
  },
  "homepage": "https://github.com/SingingApple/ShoppingList#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.18"
  }
}
